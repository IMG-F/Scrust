costume "backdrop1" "assets/stage.svg";

// Global variables visible to all sprites
public var GLOBAL_SCORE = 0;
public var GLOBAL_HIGHSCORE = 100;

// Global lists visible to all sprites
public list GLOBAL_LOG = ["Game Started"];

#[on_flag_clicked]
fn init_stage() {
    // Stage initialization logic
    GLOBAL_SCORE = 0;
    delete_all_of_list("GLOBAL_LOG");
    add_to_list("GLOBAL_LOG", "Stage initialized");
}
